{
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "linebreak-style": 0,
        "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
        "import/prefer-default-export": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "import/extensions": "off"

    },
    "env": {
      "browser": true
    },
    "extends": "airbnb-base"
}